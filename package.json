{
  "name": "@attestate/extraction-worker",
  "version": "0.7.3",
  "description": "A component that accepts JSON objects to execute simple or complex extraction maneuvres.",
  "main": "./src/worker.mjs",
  "ava": {
    "files": [
      "!test/start.mjs"
    ],
    "nodeArguments": [
      "--loader=esmock",
      "--no-warnings"
    ],
    "environmentVariables": {
      "DEBUG": "@attestate*",
      "RPC_HTTP_HOST": "https://cloudflare-eth.com"
    },
    "require": [
      "dotenv/config"
    ]
  },
  "lint-staged": {
    "*.{mjs,js,css,md}": "prettier --write"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "ava --verbose --timeout=20s",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/attestate/extraction-worker.git"
  },
  "keywords": [
    "worker",
    "requests",
    "fetch",
    "queue",
    "batch",
    "extraction"
  ],
  "author": "Tim Daubensch√ºtz <tim@daubenschuetz.de> (https://timdaub.github.io/)",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/attestate/extraction-worker/issues"
  },
  "homepage": "https://github.com/attestate/extraction-worker#readme",
  "devDependencies": {
    "ava": "4.2.0",
    "esmock": "2.2.1",
    "eth-fun": "0.9.x",
    "expressively-mocked-fetch": "0.2.3",
    "husky": "7.0.4",
    "lint-staged": "12.3.8",
    "prettier": "2.6.2"
  },
  "dependencies": {
    "@neume-network/schema": "0.8.1",
    "abort-controller": "3.0.0",
    "ajv": "8.11.0",
    "ajv-formats": "2.1.1",
    "cross-fetch": "3.1.5",
    "debug": "4.3.4",
    "dotenv": "16.0.0",
    "fastq": "1.13.0",
    "limiter": "2.0.1",
    "multiformats": "9.9.0"
  },
  "peerDependencies": {
    "eth-fun": "0.9.x"
  }
}
